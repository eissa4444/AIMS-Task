@model IEnumerable<BOL.Warehouse>

@{
    ViewBag.Title = "WareHouses";
}

<h2>The Warehouses</h2>

<p>
    @Html.ActionLink("Add a warehouse", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayName("Warehouse name")
        </th>
        <th>
            @Html.DisplayName("Setting")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.w_name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.w_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.w_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.w_id })
            </td>
        </tr>
    }

</table>
